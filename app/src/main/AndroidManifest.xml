<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.go.sport">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoSport"
        android:usesCleartextTraffic="true"
        tools:replace="android:theme"
        tools:targetApi="m">


        <activity android:name=".ui.dashboard.mypastgames.MyPastGamesActivity" />
        <activity android:name=".newui.ViewBookingSummaryActivity" />
        <activity android:name=".newui.NewBookingSummaryScreen" />
        <activity
            android:name=".ui.dashboard.fragments.home.mygroups.GroupViewMembersActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.book.sports.BookNowSportsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.train.trainsingleitem.TrainItemActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.requestfunds.RequestFundsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.transferfunds.TransferFundsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.book.hostactivity.BookNowHostActivity"
            android:screenOrientation="fullSensor" />

        <activity
            android:name=".ui.dashboard.editprofile.selectsports.SelectSportsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.hostactivity.fragments.venue.book.BookVenueActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.mygroups.create.CreateGroupActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.leaderboard.LeaderBoardActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.community.CommunityActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.offers.details.OfferDetailsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.fragments.home.offers.OffersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.fragments.home.mygroups.MyGroupsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.fragments.wallet.transfermoney.TransferMoneyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".newui.NewSummaryScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.train.TrainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.playerprofile.PlayerProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.fragments.home.mygames.MyGamesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.invites.InvitesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.chats.ConversationActivity"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="conversation" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity
            android:name=".ui.dashboard.fragments.home.hostactivity.community.SelectCommunityGroupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.fragments.home.hostactivity.HostActivityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.fragments.book.venuedetails.VenueDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.chats.ChatsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.termsconditions.TermsAndConditionsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.privacypolicy.PrivacyPolicyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.activities.ActivitiesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.contactus.ContactUsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.accountsettings.changepassword.ChangePasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.dashboard.accountsettings.AccountSettingsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.editprofile.EditProfileActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.dashboard.DashboardActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.signup.SignUpActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.signin.SignInFirstActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.signin.SignInSecondActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.walkthrough.WalkThroughActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.splash.SplashActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@mipmap/ic_launcher" />
        <!--
 Set color used with incoming notification messages. This is used when no color is set for the incoming
             notification message. See README(https://goo.gl/6BKBk7) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/purple_dark" /> <!-- [END fcm_default_icon] -->
        <!-- [START fcm_default_channel] -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" /> <!-- [END fcm_default_channel] -->
        <!-- [START firebase_service] -->
        <service android:name=".fcm.MyFirebaseMessagingService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>